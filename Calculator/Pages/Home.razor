    public List<Weapon> CompList = new List<Weapon>();

    public void addToComp(Weapon item) {
        if (CompList.Count < 4) {
            CompList.Add(item);
        }
    }

    public void removeFromComp(Weapon item) {
        if (CompList.Count > 0) {
            CompList.Remove(item);
        }   
    }

    public int getCompPrice() {
        int price = 0;
        foreach (var weapon in CompList) {
            price += weapon.price;
        }
        return price;
    }
