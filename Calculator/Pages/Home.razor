@page "/"

<PageTitle>KUSB Calculator</PageTitle>

<div class="comp">
    @foreach (var weapon in CompList)
    {
        <div class="item">
            <img src="@weapon.imgUrl" alt="@weapon.name" @onclick="() => removeFromComp(weapon)" />
            <div class="label">
                <p>@weapon.name</p>
                <p>$@weapon.price</p>
            </div>
        </div>
    }
    @for (int i = CompList.Count; i < 4; i++)
    {
        <div class="item"></div>
    }
    
</div>
<div class="total">
    @if (getCompPrice() > 100)
    {
        <h1 style="color: red;"><u>$@getCompPrice()</u></h1>
    }
    else
    {
        <h1><u>$@getCompPrice()</u></h1>
    }
</div>

<div class="space">
    <br /> 
    <h1>Weapon List</h1>

    <br /> 

    @for (int i = 50; i > 0; i -= 10)
    {
        <details>
            <summary>$@i</summary>
            <div class="weapons">
                @foreach (var weapon in WeaponList)
                {
                    @if (weapon.price == i) 
                    {
                        <div @onclick="() => addToComp(weapon)">
                            <img src="@weapon.imgUrl" alt="@weapon.name" />
                        </div>    
                    }
                }
            </div>
        </details>
    }
</div>

